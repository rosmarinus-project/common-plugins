{
  "name": "@rosmarinus/common-plugins",
  "version": "0.4.0",
  "description": "some common plugins for rosmarinus",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "rollup -c rollup.config.js -w",
    "build": "rimraf dist && rollup -c rollup.config.js",
    "release": "rosmarinus-publish publish",
    "test:unit": "jest --runInBand",
    "prepare": "husky install"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rosmarinus-project/common-plugins.git"
  },
  "keywords": [
    "plugin",
    "rollup",
    "rosmarinus"
  ],
  "license": "MIT",
  "peerDependencies": {
    "rollup": "^4.12.0"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/pluginutils": "^5.1.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@rosmarinus/eslint-config-rosmarinus": "^1.0.4",
    "@rosmarinus/local-publish-tool": "^2.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-json-reporter": "^1.2.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.9.0"
}
