{
  "name": "@rosmarinus/common-plugins",
  "version": "0.2.2",
  "description": "some common plugins for rosmarinus",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "rollup -c rollup.config.mjs -w",
    "build": "rimraf dist && rollup -c rollup.config.mjs",
    "release": "rosmarinus-publish publish",
    "test:unit": "jest --runInBand",
    "prepare": "husky install"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rosmarinus-project/common-plugins.git"
  },
  "keywords": [
    "plugin",
    "rollup",
    "rosmarinus"
  ],
  "license": "MIT",
  "peerDependencies": {
    "rollup": "^4.3.0"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@rollup/pluginutils": "^5.0.5",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-typescript": "^7.16.7",
    "@rosmarinus/eslint-config-rosmarinus": "^1.0.4",
    "@rosmarinus/local-publish-tool": "^1.0.8",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^27.4.0",
    "@types/node": "^18.14.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "jest": "^27.4.7",
    "jest-json-reporter": "^1.2.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.3.0",
    "typescript": "^5.2.2"
  },
  "packageManager": "pnpm@8.9.0"
}
